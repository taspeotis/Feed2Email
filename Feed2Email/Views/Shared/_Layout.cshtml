                    <a class="brand">Feed2Email</a>

                    @*            
                    <div class="nav-collapse">
            
                        <ul class="nav pull-right">
                    
                            <li class="">
                        
                                <a href="javascript:;"><i class="icon-chevron-left"></i> Back to Homepage</a>
                            </li>
                        </ul>
                
                    </div> <!-- /nav-collapse -->*@
                </div>
            </div>
        </div>

        @RenderBody()

